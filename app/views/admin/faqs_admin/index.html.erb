<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>BodyFrame</title>

    <link href="https://fonts.googleapis.com/css?family=Poppins:400,500,600,700,800&display=swap" rel="stylesheet">
    <style type="text/css">
.faq-holder {
    background: #011213;
    color: #fff;
    font-family: 'Poppins', sans-serif;
    padding: 40px 0;
}
*, ::after, ::before {
    box-sizing: border-box;
}
body {
    margin: 0;
    padding: 0;
}
.container {
    width: 100%;
    padding-right: 15px;
    padding-left: 15px;
    margin-right: auto;
    margin-left: auto;
}
p {
    font-size: 13px;
}
.privacy-updated {
    padding: 0;
    margin: 0;
    line-height: 26px;
    list-style: none;
}
ul.who-links {
    list-style: none;
}
td {
    color: #fff;
    font-size: 13px;
    padding: 12px;
}
li {
    line-height: normal;
    font-size: 13px;
    margin-bottom: 1em;
}
.number {
    /*font-size: 26px;*/
    padding-right: 10px;
    font-weight: 500;
}
h1 {
    font-size: 18px;
    text-transform: uppercase;
    font-weight: 500;
}
.width-20 {
    width: 20%;
    background: #ddd;
}
.width-80 {
    width: 80%;
    background: #ddd;
}
.border {
    border-bottom: 1px solid #cecece;
    padding: 0;
}
table {
    border-collapse: collapse;
        margin-bottom: 40px;
}
.table-responsive {
    max-width: 100%;
    overflow: auto;
}
a {
    color: #60b3ef;
}
.privacy-heading {
    font-size: 30px;
    padding-bottom: 30px;
    font-weight: 600;
    text-align: center;
    color: #fff;
}
.ques-holder {
    margin-bottom: 30px;
}
.ques-holder:last-child {
    margin: 0;
}
.ques-holder h3 {
    margin-bottom: 10px;
    font-size: 16px;
    text-transform: uppercase;
    font-weight: 500;
}






@media (min-width: 576px) {
  .container {
    max-width: 540px;
}
}
@media (min-width: 768px) {
  .container {
    max-width: 720px;
}
}
@media (min-width: 992px) {
  .container {
    max-width: 960px;
}
}
@media (min-width: 1200px) {
  .container {
    max-width: 1140px;
}
}



@media only screen and (max-width: 1199px) {

a {
    word-break: break-all;
}
ul.who-links {
    padding: 0;
}
}

@media only screen and (max-width: 767px) {
  .width-20, .width-80 {
    font-size: 13px;
}
.faq-holder {
    padding: 10px 0;
}
}

</style>
</head>
  <body>
    <div class="faq-holder">
      <div class="container">
        <% @faqs.each do|faq| %>
          <div class="ques-holder">
            <h3><%= faq.question %></h3>
            <p><%= faq.answer %></p>
          </div>
        <%end%>
      </div>
    </div>
  </body>
</html>